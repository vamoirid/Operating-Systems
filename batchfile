pwd 
ls 
pwd ; pwd ; pwd 
pw
pwd ; ls -l && ps -a
pw && ls
ls && ; pwd
pwd ; && pwd
echo "Hello!" > file.txt
cat file.txt

pwd
echo "Hello World" > file1.txt
cat < file1.txt > file2.txt && cat file2.txt 

cat src/myshell.c | wc -l > file3.txt && cat file3.txt ; ls

rm -f file.txt file1.txt file2.txt file3.txt
ls

quit

